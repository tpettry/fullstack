//.padstart() and .padend()example

'Turtle'.padstart(10);//will produce 10 spaces in front of Turtle

'turtle'.padend(10);//will produce 10 spaces after the Turtle

//
const fun = (a, b, c, d,) => {
  console.log(a);
}

fun(1,2,3,4,) // this will produce console.log a = 1

Object.values
Object.entries
//before we had Object.keys

let obj = {
  username0: 'Santa',
  username1: 'rudolf',
  username2: 'Mr. Grinch'
}

//before we could do this
 Object.keys(obj).forEach((key, index) => {
   console.log(key, obj[key]);
 })

 Object.values(obj).forEach(value => {
   console.log(value);
 })//this will just produce the value

 Object.entries(obj).forEach(value => {
   console.log(value);
 })// this would produce an array of the values username0: Santa

 Object.entries(obj).map(value => {
   return value[1] + value[0].replace('username', '')
 })//produces usernames example Santa0, Rudolf1
